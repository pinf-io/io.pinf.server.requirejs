{
  "name": "io.pinf.server.requirejs",
  "version": "0.0.0",
  "private": true,
  "pm": "npm",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinf-io/io.pinf.server.requirejs"
  },
  "dependencies": {
    "q": "~1.0.0",
    "glob": "~3.2.9",
    "fs-extra": "~0.8.1",
    "requirejs": "~2.1.11",
    "send": "~0.2.0",
    "smi.cli": "0.x"
  },
  "scripts": {
    "install": "./node_modules/.bin/smi install"
  },
  "upstream": {
    "packages": {
      "top": [
        "../../*",
        "../../../../*"
      ]
    }
  },
  "mappings": {
    "io.pinf.server.www": "top/io.pinf.server.www"
  },
  "config": {
    "smi.cli": {
      "packagesDirectory": "node_modules"
    },
    "pio.deploy.converter": {
      "name": "nodejs-server"
    }
  }
}
